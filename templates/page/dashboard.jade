.panel.panel-default
  .panel-body
    .col-md-8
      input#inputSuccess.form-control(type='text' placeholder="'How' Many 'Leads' Generated 'Within'")  
    .col-md-2
        .container
          .dropdown.pull-right
            button#menu1.btn.btn-primary.dropdown-toggle(type='button', data-toggle='dropdown')
              | Filter Time Span
              span.caret
            ul.dropdown-menu(role='menu', aria-labelledby='menu1')
              li(role='presentation')
                a(role='menuitem', tabindex='-1', href='#') This Month
              li(role='presentation')
                a(role='menuitem', tabindex='-1', href='#') Current Year
              li(role='presentation')
                a(role='menuitem', tabindex='-1', href='#') Last Year
              li(role='presentation')
                a(role='menuitem', tabindex='-1', href='#') Last 2 Years
    .col-md-2
      .container    
          .dropdown.pull-right
            button#menu1.btn.btn-primary.dropdown-toggle(type='button', data-toggle='dropdown')
              | Category Filter
              span.caret
            ul.dropdown-menu(role='menu', aria-labelledby='menu1')
              li(role='presentation')
                a(role='menuitem', tabindex='-1', href='#') Printing Industries
              li(role='presentation')
                a(role='menuitem', tabindex='-1', href='#') Outsourcing
              li(role='presentation')
                a(role='menuitem', tabindex='-1', href='#') Third Party
              li(role='presentation')
                a(role='menuitem', tabindex='-1', href='#') Category      

