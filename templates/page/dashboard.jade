.row.xepan-push-large
    .main-box-body.clearfix
      table.table.footable.toggle-circle-filled.default.breakpoint.footable-loaded(data-filter-text-only='true', data-filter='#filter', data-page-size='6')
        thead
          tr
            th.footable-visible.footable-first-column.footable-sortable
              | Campaign
              span.footable-sort-indicator
            th.footable-visible.footable-sortable
              | Completed On
              span.footable-sort-indicator
            th.footable-visible.footable-sortable(data-hide='phone')
              | Leads
              span.footable-sort-indicator
            th.text-center.footable-visible.footable-last-column.footable-sortable(data-hide='phone,tablet')
              | Response
              span.footable-sort-indicator
          
          tr(style='display: table-row;')
            td.footable-visible.footable-first-column
              span.footable-toggle
              a(href='#') Printing Companies (Jaipur)
            td.footable-visible
              | 2013/08/08
            td.footable-visible
              a(href='#') 900
            td.text-center.footable-visible.footable-last-column
              span.label.label-success Great
          tr(style='display: table-row;')
            td.footable-visible.footable-first-column
              span.footable-toggle
